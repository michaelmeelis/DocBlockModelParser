{
    "name": "michaelmeelis/DocBlockModelParser",
    "description": "This package will parse the doc block into DB relations",
    "keywords": [
        "docblock",
        "orm",
        "database",
        "properties"
    ],
    "homepage": "https://github.com/michaelmeelis/DocBlockModelParser",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Meelis",
            "email": "michael.meelis@gmail.com",
            "homepage": "https://github.com/michaelmeelis",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.5.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
